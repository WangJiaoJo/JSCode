
<form class="form form-horizontal" id="newNote-form">
    <div class="form-group">
    <label for="Title" class="col-sm-2 control-label">Title</label>
    <div class="col-sm-10">
        <input type="text" class="form-control" id="Title" placeholder="Title" required>
    </div>
</div>
    <div class="form-group">
        <label for="datepicker" class="col-sm-2 control-label">DueDate</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="datepicker" placeholder="yyyy/mm/dd" required>
        </div>
    </div>
    <div class="form-group">
        <label for="Summary" class="col-sm-2 control-label">Summary</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="Summary" placeholder="Summary" required>
        </div>
    </div>
    <div class="form-group">
        <label for="noteBody" class="col-sm-2 control-label">Body</label>
        <div class="col-sm-10">
            <textarea id="noteBody" class="form-control" placeholder="Body" rows="9" required></textarea>
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <button type="submit" class="btn btn-default">Submit</button>
            <button type="button" class="btn btn-default" id="backToList">Back to List</button>
        </div>
    </div>
</form>
<div class="alert alert-danger hidden" id="form-alert">
</div>